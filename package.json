  "scripts": {
    "clean": "del .build",
    "start": "node --use-strict src/server.js",
    "build": "npm run clean && webpack",
    "develop": "run-p watch:js:*",
    "watch:test": "npm run test:unit -- -w",
    "watch:js:client": "webpack --watch --progress",
    "watch:js:server": "nodemon $NODE_DEBUG_OPTION --inspect",
    "lint": "npm-run-all lint:all:*",
    "lint:all:js": "eslint ./test ./src ./assets ./config ./circleci",
    "lint:all:styles": "sass-lint -v -q -c .sass-lint.yml assets/stylesheets/**/*.scss",
    "test": "npm-run-all test:*",
    "test:unit": "NODE_ENV=test LOG_LEVEL=silent mocha ./test/unit --opts ./test/unit/mocha.opts",
    "test:acceptance": "nightwatch --config test/acceptance/nightwatch.conf.js --env default --skiptags ignore",
    "coverage": "nyc --reporter=html --reporter=text --reporter=lcov npm run test:unit",
    "circle:unit": "nyc --reporter=lcov npm run test:unit -- --reporter mocha-circleci-reporter && codeclimate-test-reporter < coverage/lcov.info",
    "circle:acceptance": "nightwatch --config test/acceptance/nightwatch.conf.js --verbose --env circleci",
    "circle:acceptance:dit-staff": "npm run circle:acceptance --  --skiptags ignore,da,lep",
    "circle:acceptance:da-staff": "npm run circle:acceptance --  --tag da --skiptags ignore",
    "circle:acceptance:lep-staff": "npm run circle:acceptance --  --tag lep --skiptags ignore",
    "heroku-postbuild": "yarn cache clean && npm rebuild && npm run build",
    "lint-staged": "lint-staged"
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.js": "eslint",
    "*.scss": "sass-lint -v -q -c .sass-lint.yml"
  },
