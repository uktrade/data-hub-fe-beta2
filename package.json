  "scripts": {
    "clean": "del .build",
    "start": "node --use-strict src/server.js",
    "build": "npm run clean && webpack",
    "develop": "run-p watch:js:*",
    "watch:test": "npm run test:unit -- -w",
    "watch:js:client": "webpack --watch --progress",
    "watch:js:server": "nodemon $NODE_DEBUG_OPTION --inspect",
    "lint": "npm-run-all lint:all:*",
    "lint:all:js": "eslint ./test ./src ./assets ./config *.js",
    "lint:all:styles": "sass-lint -c .sass-lint.yml 'src/**/*.scss' -v -q",
    "lint:modified:js": "CHANGED_FILES=$(git diff --staged --name-only -- '*.js'); if [ \"$CHANGED_FILES\" ]; then eslint $(echo $CHANGED_FILES); fi",
    "lint:modified:styles": "sass-lint -c .sass-lint.yml $(git diff --staged --name-only -- '*.scss') -v -q",
    "test": "npm-run-all test:*",
    "test:unit": "NODE_ENV=test LOG_LEVEL=silent mocha ./test/unit --opts ./test/unit/mocha.opts",
    "test:acceptance": "nightwatch --config test/acceptance/nightwatch.conf.js",
    "coverage": "nyc --reporter=html --reporter=text --reporter=lcov npm run test:unit",
    "circle": "npm run lint && nyc --reporter=lcov npm run test:unit -- --reporter mocha-circleci-reporter && npm run build && codeclimate-test-reporter < coverage/lcov.info",
    "heroku-postbuild": "npm rebuild && npm run build"
  },
  "pre-commit": [
    "lint:modified:styles",
    "lint:modified:js"
  ],
  "contributors": [
    "Ben Chidgey <ben@feedmypixel.com> (http://feedmypixel.com)",
