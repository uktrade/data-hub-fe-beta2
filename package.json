{
  "name": "data-hub-frontend",
  "description": "Data Hub Frontend",
  "main": "src/server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/uktrade/data-hub-frontend"
  },
  "scripts": {
    "clean": "del .build",
    "start": "node --use-strict src/server.js",
    "build": "npm run clean && webpack",
    "develop": "NODE_ENV=development run-p watch:js:*",
    "watch:test": "npm run test:unit -- -w",
    "watch:js:client": "webpack --watch --progress",
    "watch:js:server": "nodemon $NODE_DEBUG_OPTION --inspect --ignore 'src/**/__test__/**/*'",
    "lint": "run-p lint:*",
    "lint:js": "eslint --ext .jsx,.js ./test ./src ./assets ./.circleci",
    "lint:sass": "sass-lint -v -q -c .sass-lint.yml assets/stylesheets/**/*.scss",
    "sandbox": "sandbox --watch=true --base=./test/sandbox run",
    "test": "run-s test:*",
    "test:unit": "NODE_ENV=test API_ROOT=http://test LOG_LEVEL=silent mocha ./src/**/*.test.js --opts ./test/unit/mocha.opts",
    "test:unit-client": "NODE_ENV=test mocha-webpack --webpack-config webpack.config.js --opts ./test/unit-client/mocha.opts ./test/unit-client/assets/javascripts/**/*.test.js",
    "test:acceptance": "nightwatch --config test/acceptance/nightwatch.conf.js --env default --skiptags ignore",
    "test:acceptance:remote": "./.circleci/scripts/local-runner.js --config test/acceptance/nightwatch.conf.js",
    "test:functional": "cypress run",
    "test:functional:watch": "cypress open",
    "test:e2e:lep": "CYPRESS_integrationFolder=test/end-to-end/cypress/specs/LEP cypress run --config baseUrl=http://localhost:3000",
    "test:e2e:da": "CYPRESS_integrationFolder=test/end-to-end/cypress/specs/DA cypress run --config baseUrl=http://localhost:3000",
    "test:e2e:dit": "CYPRESS_integrationFolder=test/end-to-end/cypress/specs/DIT cypress run --config baseUrl=http://localhost:3000",
    "test:e2e:watch": "CYPRESS_integrationFolder=test/end-to-end/cypress/specs cypress open --config baseUrl=http://localhost:3000",
    "test:visual": "./node_modules/.bin/wdio test/visual/wdio.conf.js",
    "test:visual:update": "wdio-image-diff -u",
    "coverage": "nyc --reporter=html --reporter=text --reporter=lcov npm run test:unit",
    "circle:unit": "nyc --reporter=lcov npm run test:unit -- --reporter mocha-circleci-reporter && codeclimate-test-reporter < coverage/lcov.info",
    "circle:unit-client": "npm run test:unit-client --reporter mocha-circleci-reporter",
    "circle:acceptance": "nightwatch --config test/acceptance/nightwatch.conf.js --env circleci",
    "circle:acceptance:dit-staff": "yarn circle:acceptance --skiptags ignore,da,lep",
    "circle:acceptance:da-staff": "yarn circle:acceptance --tag da --skiptags ignore",
    "heroku-postbuild": "yarn cache clean && npm rebuild && npm run build",
    "lint-staged": "lint-staged"
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.{js,jsx}": "eslint",
    "*.scss": "sass-lint -v -q -c .sass-lint.yml"
  },
  "license": "MIT",
